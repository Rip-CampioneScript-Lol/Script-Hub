local NotificationService = game:GetService("StarterGui")
NotificationService:SetCore("SendNotification", {
    Title = "‚ö° Anti-Hambreado Script Hub",
    Text = "üí¨ discord.gg/rTGF5xhe3h",
    Duration = 10
})

local function copyToClipboard(text)
    setclipboard(text)
    NotificationService:SetCore("SendNotification", {
        Title = "Enlace copiado",
        Text = "Este archivo debe ser descargado para ser ejecutado, el link directo se copi√≥ a tu portapapeles",
        Duration = 5
    })
end

local function safeExecute(name, codeOrUrl)
    local success, err = pcall(function()
        if string.find(codeOrUrl, "loadstring") then
            loadstring(codeOrUrl)()
        elseif string.find(codeOrUrl, "mediafire.com") or string.find(codeOrUrl, "pastebin.com") then
            copyToClipboard(codeOrUrl)
        else
            loadstring(game:HttpGet(codeOrUrl))()
        end
    end)
    if not success then
        warn("‚ùå Error al ejecutar script:", name, "| Error:", err)
        NotificationService:SetCore("SendNotification", {
            Title = "Script fall√≥",
            Text = "El script '" .. name .. "' fall√≥ al ejecutarse.",
            Duration = 5
        })
    end
end

