local sortedGames = {}
for gameName, _ in pairs(tabs) do
    table.insert(sortedGames, gameName)
end
table.sort(sortedGames)

for _, gameName in ipairs(sortedGames) do
    local tab = Window:CreateTab(gameName)
    for _, script in ipairs(tabs[gameName]) do
        tab:CreateButton({
            Name = script[1],
            Callback = function()
                safeExecute(script[1], script[2])
            end
        })
    end
end
